<!-- USER BORROWED DEVICES LIST POPUP/MODAL -->
<div class="modal fade" id="userBorrowedDevicesModal" tabindex="-1" role="dialog" aria-labelledby="userBorrowedDevicesModalTitle" aria-hidden="true">
	<div class="modal-dialog modal-dialog-centered modal-dialog-scrollable" role="document">
		<div class="modal-content">
			<div class="modal-header">
				<% if(searchUser != undefined) {  %>
					<h5 class="modal-title" id="userBorrowedDevicesModalTitle">Liste du matérial emprunté par <%= searchUser.firstName%> <%= searchUser.lastName%></h5>
				<% } %>
				<button type="button" class="close" data-dismiss="modal" aria-label="Close">
					<span aria-hidden="true">&times;</span>
				</button>
			</div>
			<div class="modal-body">
				<table class="table table-bordered shadow-sm">
					<thead>
						<tr>
							<th class="col">Nom</th>
							<th class="col">Version</th>
							<th class="col">Référence</th>
							<th class="col">Numéro</th>
							<th class="col">Date de début d'emprunt</th>
							<th class="col">Date de fin d'emprunt</th>
						</tr>
					</thead>
					<tbody>
						<!-- A FAIRE EN EXPRESS-->
						<!-- liste du matériel emprunté pour un utilisateur donné-->
						<tr>
							<td>a</td>
							<td>a</td>
							<td>a</td>
							<td>a</td>
							<td>a</td>
							<td>a</td>
						</tr>
						<!-- FIN DE A FAIRE EN EXPRESS-->
					</tbody>
				</table>
			</div>

			<div class="modal-footer">
				<button type="button" class="btn btn-secondary" data-dismiss="modal">Fermer</button>
			</div>
		</div>
	</div>
</div>